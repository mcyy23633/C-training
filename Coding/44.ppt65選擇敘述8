#include <stdio.h>
main()
{
	int N;
	char G;
	printf("Please input your score:");
	scanf("%d", &N);
	switch (N/10)
	{
		case 10:
		case 9:
			G='A';
			break;
		case 8:
			G='B';
			break;
		case 7:
			G='C';
			break;
		case 6:
			G='D';
			break;
		default:
			G='F';
	}
	printf("You get:%c", G);
}

